<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.TableColumn?>

<VBox xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
  <TableView fx:id="table" VBox.vgrow="ALWAYS">
    <columns>
      <TableColumn fx:id="indexCol" text="ID"/>
      <TableColumn fx:id="nameCol" text="Name"/>
      <TableColumn fx:id="emailCol" text="Email"/>
      <TableColumn fx:id="phoneCol" text="Mobile"/>
      <TableColumn fx:id="addressCol" text="Address"/>
      <TableColumn fx:id="tagsCol" text="Tags"/>
      <TableColumn fx:id="notesCol" text="Notes"/>
      <TableColumn fx:id="eventsCol" text="Events"/>
    </columns>
  </TableView>
</VBox>
